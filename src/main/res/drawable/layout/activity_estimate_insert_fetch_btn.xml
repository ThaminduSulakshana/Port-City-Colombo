<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EstimateInsertFetchBtn">


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="357dp"
        android:layout_height="111dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="38dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_f" />

    <com.google.android.material.bottomappbar.BottomAppBar
        android:id="@+id/bottomAppBar8_1"
        style="@style/Widget.MaterialComponents.BottomAppBar.Colored"
        android:layout_width="387dp"
        android:layout_height="195dp"
        android:layout_gravity="bottom"
        android:layout_marginStart="13dp"
        android:layout_marginTop="220dp"
        android:layout_marginEnd="9dp"
        app:backgroundTint="#434DA4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="BottomAppBar" />

    <Button
        android:id="@+id/btnInsert"
        android:layout_width="198dp"
        android:layout_height="54dp"
        android:layout_marginTop="244dp"
        android:text="Request A Residential PLot"
        android:textColor="#2196F3"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/bottomAppBar8_1"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="@+id/bottomAppBar8_1"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.bottomappbar.BottomAppBar
        android:id="@+id/bottomAppBar8_2"
        style="@style/Widget.MaterialComponents.BottomAppBar.Colored"
        android:layout_width="390dp"
        android:layout_height="169dp"
        android:layout_gravity="bottom"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="116dp"
        app:backgroundTint="#D71B5F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="BottomAppBar,DuplicateIds" />

    <Button
        android:id="@+id/btnFetch"
        android:layout_width="207dp"
        android:layout_height="54dp"
        android:text="Show my Requested Details"
        android:textColor="#2196F3"
        android:textSize="15sp"
        android:textStyle="bold"
        app:backgroundTint="#a30b42"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.519"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.695" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="284dp"
        android:layout_height="47dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="16dp"
        android:text="If you need to get plot,you can select and request using this App "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

</androidx.constraintlayout.widget.ConstraintLayout>